{
  "expo": {
    "name": "SHchatbot",
    "slug": "SHchatbot",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "android": {
      "package": "com.shinhan.chatbot",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "@react-native-firebase/app",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a3d87e54-80f8-4ad0-a442-d88ec31a5a5e"
      }
    },
    "ios": {
      "bundleIdentifier": "com.shinhan.chatbot",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "updates": {
      "url": "https://u.expo.dev/a3d87e54-80f8-4ad0-a442-d88ec31a5a5e"
    }
  }
}
